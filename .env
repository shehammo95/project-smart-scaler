REACT_APP_AUTH_USER_KEY=auth_user

# Data Collection APIs
USDA_API_KEY=your-usda-api-key-here
OPENFOODFACTS_API_KEY=your-openfoodfacts-api-key-here

# Docker Configuration
DOCKER_IMAGE_NAME=food-scale
DOCKER_IMAGE_TAG=latest 